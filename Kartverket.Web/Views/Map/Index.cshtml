@model MapIndexModel
@{
    ViewBag.Title = "Map";
    
}

<div id="map" style="position: absolute; top: 0; left: 0; width: 100vw; height: 100vh; margin: 0; padding: 0;"></div>

@section Scripts {
    <script src="~/js/map/index.js"></script>
    <script>
        window.addEventListener('load', function () {
            const geojson = @Html.Raw(Model.GeoJson);
            
            window.Map.load(geojson, document.getElementById('map'));
        });
    </script>
}
